CREATE TABLE movies (
  id INTEGER PRIMARY KEY NOT NULL,
  title STRING NOT NULL,
  genre_id INTEGER NOT NULL REFERENCES genres,
  released INTEGER NOT NULL,
  rating FLOAT DEFAULT NULL,
