Query: 
        SELECT m.title, g.name AS genre, s.name AS studio
        FROM movies m
            INNER JOIN genres g ON m.genre_id = g.id AND g.id = 1
            INNER JOIN studios s ON m.studio_id = s.id AND s.id = 4

